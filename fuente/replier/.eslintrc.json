{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/no-unresolved": [
      2,
      { "caseSensitive": true }
   ]
  },
  "ignorePatterns": ["dist/*", "node_modules/*"]
}
